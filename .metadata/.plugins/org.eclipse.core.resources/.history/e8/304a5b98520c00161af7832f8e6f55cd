package sk.etape3;

import org.junit.Test;

import junit.framework.Assert;

public class TestEtape3 {

	@Test
	public void testA() {
		I i = new B();
		A a = new A(i);
		Assert.assertTrue(a.m());
	}

	@Test
	public void testB() {
		B b = new B();
		Assert.assertTrue(b.q());
	}
	@Test
	public void testB2() {
		B2 b = new B2();
		Assert.assertTrue(b.q());
	}

}
